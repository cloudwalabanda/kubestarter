apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginxwala
spec:
  replicas: 4
  selector:
    matchLabels:
      app: mylabel

  template:
    metadata:
      labels:
        app: mylabel

    spec:
      containers:
        - name: mycontainer
          image: nginx
          ports:
            - containerPort: 80
